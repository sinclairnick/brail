{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "React Library",
  "compilerOptions": {
    "module": "ESNext",
    "moduleResolution": "node",
    "composite": false,
    "declaration": true,
    "declarationMap": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "inlineSources": false,
    "isolatedModules": true,
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "preserveWatchOutput": true,
    "skipLibCheck": true,
    "strict": true,
    "allowSyntheticDefaultImports": true,
    "jsx": "react-jsx",
    "lib": [
      "ES2017",
      "DOM"
    ],
    "target": "ES2017",
    "outDir": "./dist",
    "emitDeclarationOnly": false,
    "incremental": true,
    "tsBuildInfoFile": "/Users/nick/Documents/code/brail/node_modules/.pnpm/vitest@0.29.7/node_modules/vitest/dist/tsconfig.tmp.tsbuildinfo"
  },
  "exclude": [
    "dist",
    "build",
    "node_modules",
    "dist"
  ],
  "include": [
    "./src"
  ]
}