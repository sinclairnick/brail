{
  "name": "brail",
  "version": "1.0.0-alpha.5",
  "description": "",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "private": false,
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "default": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./sdk": {
      "import": "./dist/sdk.mjs",
      "require": "./dist/sdk.js",
      "default": "./dist/sdk.js",
      "types": "./dist/sdk.d.ts"
    },
    "./trpc": {
      "import": "./dist/trpc.mjs",
      "require": "./dist/trpc.js",
      "default": "./dist/trpc.js",
      "types": "./dist/trpc.d.ts"
    },
    "./next": {
      "import": "./dist/next.mjs",
      "require": "./dist/next.js",
      "default": "./dist/next.js",
      "types": "./dist/next.d.ts"
    },
    "./next-loader": {
      "import": "./dist/next-loader.mjs",
      "require": "./dist/next-loader.js",
      "default": "./dist/next-loader.js",
      "types": "./dist/next-loader.d.ts"
    },
    "./devtools": {
      "import": "./dist/devtools.mjs",
      "require": "./dist/devtools.js",
      "default": "./dist/devtools.js",
      "types": "./dist/devtools.d.ts"
    },
    "./*": {
      "import": "./dist/*.mjs",
      "require": "./dist/*.js",
      "default": "./dist/*.js",
      "types": "./dist/*.d.ts"
    }
  },
  "scripts": {
    "test": "jest spec",
    "build": "tsup-node",
    "dev": "tsup-node --watch",
    "check": "tsc --noEmit"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@brail/react": "workspace:*",
    "@brail/types": "workspace:*",
    "@trpc/client": "^10.11.1",
    "@trpc/server": "^10.11.1",
    "@types/jest": "^29.4.0",
    "@types/node": "^18.14.2",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.10",
    "@types/webpack-sources": "^3.2.0",
    "jest": "^29.4.2",
    "next": "^13.2.1",
    "react": "^18.2.0",
    "react-dom": "18.2.0",
    "trpc-openapi": "^1.1.2",
    "ts-jest": "^29.0.5",
    "tsconfig": "workspace:*",
    "typescript": "^4.9.5",
    "webpack": "^5.75.0"
  },
  "dependencies": {
    "@brail/devtools": "workspace:*",
    "zod": "^3.20.6"
  }
}
